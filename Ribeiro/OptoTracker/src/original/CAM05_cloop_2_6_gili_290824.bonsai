                <cv:Pivot>
                  <cv:X>704</cv:X>
                  <cv:Y>704</cv:Y>
                </cv:Pivot>
                <cv:Translation>
                  <cv:X>0</cv:X>
                  <cv:Y>0</cv:Y>
                </cv:Translation>
                <cv:Rotation>3.14159274</cv:Rotation>
                <cv:Scale>
                  <cv:X>1</cv:X>
                  <cv:Y>1</cv:Y>
                </cv:Scale>
              </Combinator>
            </Expression>
